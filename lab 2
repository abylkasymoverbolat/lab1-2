a = int(input("Ерболат: ")) #имя студента
if(a >= 90): # условие if
    print("Уровень: A")
elif(a >= 80 & a < 90): # если оценка ровно 80 но меньше 90
    print("Уровень: B") # уровень B
elif(a >= 70 & a< 80):
    print("Уровень: C")
elif(a >= 60 & a < 70):
    print("Уровень: D")
else:
    print("Уровень: F")